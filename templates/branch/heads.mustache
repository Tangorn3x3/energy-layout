<script id="headsTpl" type="text/html">
    <section id="branch">
        <div class="container">
            <div class="row">
                {{#heads}}
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 member">
                        <div class="col-lg-12 photo"><img src="{{photo}}" alt=""></div>
                        <div class="col-lg-12  name">{{name}}</div>
                        <div class="col-lg-12  position">{{position}}</div>
                        <div class="col-lg-12  meeting-cont">
                            <div class="title">Прием граждан</div>
                            <div class="schedule">
                                <div class="period">{{meeting.period}} <strong>{{meeting.day}}</strong></div>
                                <div class="time">c {{meeting.from}} до {{meeting.to}}</div>
                            </div>
                            <div class="enroll"><a href="#">Записаться на прием</a></div>
                        </div>
                    </div>
                {{/heads}}
            </div>
        </div>
    </section>
</script>